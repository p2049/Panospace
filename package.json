{
  "name": "panospace",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "description": "Professional social platform and marketplace for photographers and visual artists",
  "author": "Panospace Team",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 1000",
    "lint:fix": "eslint src --ext ts,tsx,js,jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "echo \"Playwright E2E tests disabled\"",
    "test:e2e:ui": "echo \"Playwright UI disabled\"",
    "emulators": "firebase emulators:start",
    "deploy": "firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "create-demo-account": "node scripts/createDemoAccount.js",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@stripe/stripe-js": "^2.4.0",
    "countries-list": "^3.2.2",
    "country-region-data": "^4.0.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "exifr": "^7.1.3",
    "firebase": "^10.7.1",
    "framer-motion": "^12.23.25",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^9.0.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.68.0",
    "react-i18next": "^16.4.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.20.1",
    "react-window": "^2.2.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "firebase-admin": "^13.6.0",
    "jsdom": "^23.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^5.0.8",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}